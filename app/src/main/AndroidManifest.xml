<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="br.com.talkmessenger">

    <application
        android:allowBackup="false"
        android:fullBackupContent="false"
        android:icon="@drawable/ic_icons8_bt21_rj"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_icons8_bt21_rj"
        android:supportsRtl="true"
        android:theme="@style/Theme.TalkMessenger">
        <activity android:name=".ui.SignUpActivity">
            <intent-filter>
                <category android:name="android.intent.category.LAUNCHER" />
                <action android:name="android.intent.action.MAIN" />
            </intent-filter>
        </activity>
        <activity android:name=".ui.OtpActivity" />
        <activity android:name=".ui.VerifyPhoneActivity" />
        <activity android:name=".ui.IntroActivity" />
        <activity android:name=".MainActivity" />
    </application>

</manifest>